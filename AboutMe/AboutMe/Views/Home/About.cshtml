@using AboutMe.Models
@model IEnumerable<AboutMe.Models.RssItem>
@{
    ViewBag.Title = "About";
}


<h1>Pictures</h1>

<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <div class="carousel-inner">
        <div class="item active">
            <img src="https://farm8.staticflickr.com/7847/32338072957_d0075017e9_b.jpg" alt="">
            <div class="carousel-caption">
                <h3>ROKIT WILLIAMS RACING FW42</h3>
                <p>Robert Kubica behind wheel in pre-season testing in Barcelona 2019</p>
            </div>
        </div>
        <div class="item">
            <img src="https://farm8.staticflickr.com/7900/32338073037_68258d3f91_b.jpg" alt="">
            <div class="carousel-caption">
                <h3>Scuderia Ferrari</h3>
                <p>Sebastian Vettel behind wheel in pre-season testing in Barcelona 2019</p>
            </div>
        </div>

        <div class="item">
            <img src="https://farm8.staticflickr.com/7874/33404175068_cc36ca4291_b.jpg" alt="New York">
            <div class="carousel-caption">
                <h3>Mercedes-AMG Petronas Motorsport</h3>
                <p>valtteri bottas behind wheel in pre-season testing in Barcelona 2019</p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<h1>Hobby & Passions </h1>
<div id="Hobby" class="container-fluid">
    <h3 class="	glyphicon glyphicon-pushpin"> <a href="https://www.f1calendar.com">Formula 1</a> </h3>
    <br>
    <h3 class="glyphicon glyphicon-cd"> <a href="https://open.spotify.com/playlist/37i9dQZF1DX76t638V6CA8"> Music</a></h3>
    <br>
    <h3 class="glyphicon glyphicon-sunglasses"> <a href="https://mapa-turystyczna.pl/route/5l3z"> Mountains</a></h3>
</div>

<h1>News</h1>
@foreach (var rssItem in Model)
{
    <h4>@rssItem.Title</h4>
    <span>@rssItem.PubDate</span>
    <p>@Html.Raw(rssItem.Description)</p>
}
<h1>Google Map</h1>

<div id="googleMap" style="width:100%;height:400px;"></div>
<script>
    function myMap() {
        var mapProp = {
            center: new google.maps.LatLng(49.7787272, 19.0566255),
            zoom: 5,
        };
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCf6UaD_CENTloQMhYmD46aEH1Had2yQ9c&callback=myMap"></script>



